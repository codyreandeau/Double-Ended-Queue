public class Node {
  
  private String data;
  private Node next;
  private Node prev;
  
  public Node(String data) {
    this.data = data;
    this.next = null;
  }
  
  public Node(String data, Node next) {
    this.data = data;
    this.next = next;
    
  }
  
  public Node(String data, Node next, Node prev) {
    this.data = data;
    this.next = next;
    this.prev = prev;
  }
}